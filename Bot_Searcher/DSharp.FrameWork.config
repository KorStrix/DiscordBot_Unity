<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    
    <sectionGroup name="applicationSettings" 
                  type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
     
      <section name="DSharp.FrameWork.Properties.Settings" 
               type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" 
               requirePermission="false" />
    
    </sectionGroup>
  
  </configSections>
  
  <applicationSettings>
  
    <DSharp.FrameWork.Properties.Settings>
      
      <!-- Defines if the application is in TestMode -->
      <setting name="TestMode" serializeAs="String">
        <value>False</value>
      </setting>

      
      <!-- ###### Event Logging ######## -->

      <!-- The name of the Eventlog, this can also be an user defined name. 
           Be careful the first 8 characters are unique for an eventlogname -->
      <setting name="EventLogName" serializeAs="String">
        <value>Application</value>
      </setting>
      
      <!-- The name of the EventSource, usually the name of the application -->
      <setting name="EventSource" serializeAs="String">
        <value>MyEventSource</value>
      </setting>

      <!-- ###### File Logging ######## -->

      <!-- Is the log active or not -->
      <setting name="Log_Active" serializeAs="String">
        <value>True</value>
      </setting>

      <!-- The filename for the log (without the extension) -->
      <setting name="Log_OutputFileName" serializeAs="String">
        <value>MyLogFileName</value>
      </setting>

      <!-- Location of the LogFile -->
      <setting name="Log_OutputDir" serializeAs="String">
        <value>D:\Shared.Data\Logs\</value>
      </setting>

      <!-- Write every x hours the eventlog, if 0 then it's every day -->
      <setting name="Log_FileHour" serializeAs="String">
        <value>0</value>
      </setting>

      <!-- None = 0, Error = 1, Warning = 2, Information = 4 -->
      <setting name="Log_VerboseLevel" serializeAs="String">
        <value>4</value>
      </setting>

      
      <!-- ###### Security ######## -->

      <!-- Store the default Key, when no key is provided in code -->
      <setting name="EncryptionServerKey" serializeAs="String">
        <value>ServerKey</value>
      </setting>

      <!-- Store the default Key, when no key is provided in code -->
      <setting name="EncryptionClientKey" serializeAs="String">
        <value>ClientKey</value>
      </setting>

    </DSharp.FrameWork.Properties.Settings>
  
  </applicationSettings>

</configuration>